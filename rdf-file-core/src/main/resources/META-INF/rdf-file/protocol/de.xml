<protocol name="de">
	<head>
		<row>
			<column>
				<output>${column.desc()}</output>
				<output>:</output>
				<output>${column.value()}</output>
			</column>
		</row>
		<row output="${columnInfo.horizontal(body,desc)}" />
	</head>
	<body>
		<row>
			<column>
				<output>${column.value()}</output>
			</column>
		</row>
	</body>
	<tail>
		<row>
			<column>
				<output>${column.value()}</output>
			</column>
		</row>
	</tail>
</protocol>